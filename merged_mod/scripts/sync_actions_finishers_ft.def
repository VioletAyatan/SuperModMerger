		SyncAction("FinisherKnuckles01")
        {
            Conditions()
            {
                Tags("!zombie_boss", "!ForceUnarmedFinisher");
                ActorType(1, "!Skullface");
				EquippedWeaponTypeRequired(0, "knuckles");
				EquippedWeaponStatsTypeRequired(0, "Blunt");
            }
			
			SoundEvents()
			{
				ExitSoundEvent("set_takedown_out");
			}

            Events()
            {
				Event(ANIM_EVENT, 0, SYNCACTION_AI_BODY_DAMAGE)
                {
                    // Fake damage to attacker to damage his head
                    DealDamage(0, 1, "finisher_body_hole@sync_action_damage_presets.scr")
                    {
                        DamageHelper(0, "R_HAND");
                        UseInstigatorForward();
                    }
                }
				
				Event(ANIM_EVENT, 0, SYNCACTION_APPLY_BLOOD_EFFECT)
                {
                    ApplyBloodEffect(0, FINISHER_BLOOD_EFFECT_AMOUNT, "Both");
                }

                Event(ANIM_EVENT, 0, SYNCACTION_TRIGGER_DEATH)
                {
                    PerformAction(0, "Finisher");
                    AddStamina(0, FINISHER_ADD_STAMINA_VALUE, true);
                    FreezeStamina(0, FINISHER_FREEZE_STAMINA_DURATION);
                    DealDamage(0, 1, "finisher_kill_ragdoll@sync_action_damage_presets.scr")
                    {
                        UseActiveWeaponId(true);
                    }
                }
            }
			
			

            States()
            {
                StartState("FinisherKnuckles01Begin");

                State("FinisherKnuckles01Begin")
                {
					StaminaCost(0, 0.5);
                    IsSpecialMove();
                    Animation(0, "m_fpp_syncaction_finisher_knuckles_attacker_01_dlc2_begin");
                    Animation(1, "m_hmf_syncaction_finisher_knuckles_victim_01_dlc2", 0.2);

                    AnimationScript(1, "sync_action_anims_ai_ft.scr");

                    RequiredActors(0, 1);
					ShowWeapons(0);

                    FinisherPhysicsSetting(1);

                    BlendPosition(0, 0, 0.2, 0.1, true);
                    BlendDirection(0, 0, 0.2, 0.1, true);

                    BlendPosition(1, 0, 0.2, 0.1, true);
                    BlendDirection(1, 0, 0.2, 0.1, true);

                    BlendLookDirection(0.2);

                    ActionCenter(1);
                    ActionCenterDirection(0,1);
                    ActionCenterLiveErrorCorrection();

                    EnableInvulnerability(0);
					BlockBreakWhenDamaged(0, 1);

                    EndTransition()
                    {
                        AnimationFinished(0);
                    }
                }
            }
        }
		
