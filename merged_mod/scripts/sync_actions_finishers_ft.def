		SyncAction("FinisherUnarmed02")
        {
            Conditions()
            {
                Tags("!zombie_boss", "!ForceUnarmedFinisher");
                ActorType(1, "!Skullface");
				EquippedWeaponTypeRequired(0, "fists", "#OpportunityWeapon");
				DirectionOffset(0, 1, 180, 90);
            }
			
			SoundEvents()
			{
				ExitSoundEvent("set_takedown_out");
			}

            Events()
            {
				Event(ANIM_EVENT, 0, SYNCACTION_AI_BODY_DAMAGE)
                {
                    // Fake damage to attacker to damage his head
                    DealDamage(0, 1, "finisher_limb_destroy@sync_action_damage_presets.scr")
                    {
                        DamageHelper(0, "DamageHelper");
                        UseInstigatorForward();
                    }
                }

                Event(ANIM_EVENT, 0, SYNCACTION_TRIGGER_DEATH)
                {
                    PerformAction(0, "Finisher");
                    AddStamina(0, FINISHER_ADD_STAMINA_VALUE, true);
                    FreezeStamina(0, FINISHER_FREEZE_STAMINA_DURATION);
                    DealDamage(0, 1, "finisher_kill_ragdoll@sync_action_damage_presets.scr");
                }
            }

            States()
            {
                StartState("FinisherUnarmed02Begin");

                State("FinisherUnarmed02Begin")
                {
					StaminaCost(0, 0.5);
                    IsSpecialMove();
                    Animation(0, "m_fpp_syncaction_finisher_unarmed_attacker_02_begin");
                    Animation(1, "m_hmf_syncaction_finisher_unarmed_victim_02", 0.2);

                    AnimationScript(1, "sync_action_anims_ai.scr");

                    RequiredActors(0, 1);

                    //FinisherPhysicsSetting(1);

                    BlendPosition(0, 0, 0.2, 0.1, true);
                    BlendDirection(0, 0, 0.2, 0.1, true);

                    BlendPosition(1, 0, 0.2, 0.1, true);
                    BlendDirection(1, 0, 0.2, 0.1, true);

                    BlendLookDirection(0.2);

                    ActionCenter(1);
                    ActionCenterDirection(0,1);
                    ActionCenterLiveErrorCorrection();

                    EnableInvulnerability(0);
					BlockBreakWhenDamaged(0, 1);

                    EndTransition()
                    {
                        AnimationFinished(0);
                    }
                }
            }
        }
		
		
