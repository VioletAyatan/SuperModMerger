    Group("FinisherStagger")
    {
        Category("Combat");
        
        SyncAction("Skullface_Front")
        {
            GrantBackImmunity()
            {
            }
            
           
            Events()
            {
                Event(ANIM_EVENT, 0, ANIM_EVENT_GIVE_GRAB_DAMAGE)
                {
                    DealDamage(0, 1, "skullface_finisher_light_hit@sync_action_damage_presets.scr");
                }
                Event(ANIM_EVENT, 0, SYNCACTION_TRIGGER_DEATH)
                {
                    DealDamage(0, 1, "skullface_finisher_heavy_hit@sync_action_damage_presets.scr");
                }
                Event(ANIM_EVENT, 1, SYNCACTION_SPAWN_FX)
                {
                    SpawnPrefab("decal3d.prefab", "Decal;Blood_splat_v5;")
                    {
                        SpawnLocation(0, "FXHelper");
                        ScaleOverride(0.5);
                    }
                }				
            }

            Conditions()
            {
               ActorType(1, "Skullface");
               DirectionOffset(0, 1, 180, 45);
			}
			
			SoundEvents()
			{
				ExitSoundEvent("set_takedown_out");
			}
			
            States()
            {
                StartState("SkullfaceFrontBegin");

                State("SkullfaceFrontBegin")
                {
					EnableInvulnerability(0);
                    IsSpecialMove();
                    RequiredActors(0, 1);
					ShowWeapons(0);
					ShowWeapons(1);

                    Animation(0, "sync_action_player_beatingup_start");
                    Animation(1, "sync_action_skullface_beatingup_start", 0.2);

                    AnimationScript(1, "sync_action_anims_ai.scr");

                    BlendPosition(0, 0, 0.05, 0.75);
                    BlendDirection(0, 0, 0.05, 0.75);
                    BlendLookDirection(0.2);

                    BlendPosition(1, 0, 0.05, 0.75);
                    BlendDirection(1, 0, 0.05, 0.75);

                    ActionCenter(1);
					ActionCenterDirection(1, 0);
                    ActionCenterLiveErrorCorrection();


                    Transition("SkullfaceFrontEnd")
                    {
                        AnimationFinished(0);
                    }

                }
                State("SkullfaceFrontEnd")
                {
					EnableInvulnerability(0);
                    RequiredActors(0, 1);
					ShowWeapons(0);
					ShowWeapons(1);

                    Animation(0, "sync_action_player_beatingup_end");
                    Animation(1, "sync_action_skullface_beatingup_end", 0.2);

                    AnimationScript(1, "sync_action_anims_ai.scr");

                    ActionCenter(1);
                    MaintainLookDirection(true);					
					
                    EndTransition()
                    {
                        AnimationFinished(0, 1);
                    }
                }
			}
		}
