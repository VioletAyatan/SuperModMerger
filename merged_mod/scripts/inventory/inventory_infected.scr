sub Viral_Spit_Def(string weapon_name="", string damage_name="")
{
    Item(weapon_name, CategoryType_Throwable)
    {
        Name("&AI_Throwable_Knife_N&");
        Description("&AI_Throwable_Knife_D&");
        ItemType(ItemType_ThrowableKnife);
        OwnerType(OwnerType_AI);
		HitReactionType(EReactionType_Cut);
		HitReactionHitSeverity(EHitSeverity_Light);
        HitReactionMoveSeverity(EMoveSeverity_Light);
		HitReactionFlags(EReactionFlag_Interrupt);	
        Visibility(true);
        MaxStackCount(1);
        Price(5);
        Mesh("wn_throwing_knife_a.msh");
        Skin("default");
        AnimPrefix("Decoy");
        AiHitSound("spit");
        PhysicsScript("simple.phx");
        HudIcon("jar");
        DestroyableDamage(20.0);
        Damage(5.0);
        ThrowSpeed(26.0);
        ExplodeDelay(10.0);
        ExplodeOnCollision(false);
        PhysicsDamageMul(5.0);
        ThrowImpulseLook(13.0);
        ThrowImpulseUp(2.0);
        ThrowBeginSound("");
        StatsType(StatsType_None);
        NoiseType("NONE");
        Color(Color_White);
        FF_EffectName("Throw_Knife");
        ThrowFlySound("ai_knife_throw_fly.wav");
        AIHandElement("L_HandHolder");
        MotionTrailFx("spitter_trail_01_a.gpufx");
		SweepSize(0.35, 0.21, 0);
		BuffPresetToApplyOnHit("spitter_toxic");
		DamageName(damage_name);
		SoundInjuryDamageType("toxic_spitter");
        ApplyLegendLevelOrNewGamePlusModifiers(false);
		GameVersion(1);
    }
}

