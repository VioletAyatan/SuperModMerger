sub Demolisher_Rubble_Def(string weapon_name="", string damage_name="", string skin="", string trail_fx="", int game_version=1)
{
    Item(weapon_name, CategoryType_Throwable)
    {
        ItemType(ItemType_ThrowableRock);
        OwnerType(OwnerType_AI);
        Visibility(true);       
        Mesh("armored_rock.msh");
        Skin(skin);
        PhysicsScript("simple.phx");     
        GetShowExplosiveHud(false);       
        DamageName(damage_name);
        DamageRange(2.0);    
        ThrowSpeed(20.0);
        DestroyableDamage(120.0);
		ScheduleExplodeAfterContact(true);		
        DamagerTakesNoDamagePlaysReactionAnim(true);
        ExplosionFx("demolisher_stone_impact.gpufx");
        ExplosionSound("demolisher_heavy_stone_explosion");
        ExplodeDelay(0.0);
        Force(200.0); 
        PhysicsDamageMul(3.0);                  
        MotionTrailFx(trail_fx);
		ThrowFlySound("demolisher_heavy_object_flying");
        AIHandElement("R_HandHolder");
        InWorldScale(2.0);
		NoiseDataUid("demolisher_rock");
		SoundInjuryDamageType("blunt_heavy");
        RagdollBehaviorPrefix("Stick2HeavyBlunt");
        DamageType(DamageType_Blast);
        HitReactionType(EReactionType_Blast);
        // NOTE: HitReactionContext is overrided by categoryType_Throwable in ItemDescWeapon::OnDefinitionsLoaded
        HitReactionHitSeverity(EHitSeverity_Heavy);
        HitReactionMoveSeverity(EMoveSeverity_Heavy);
        HitReactionFlags(EReactionFlag_Knockdown);
		HitReactionFlags(EReactionFlag_Ragdoll);
		HitReactionFlags(EReactionFlag_Interrupt);
		LimbsCutProb(0.5);
        LimbsDestroyProb(0.5);
		FullBodyCutProb(0.25);
		GameVersion(1);
    }
}

